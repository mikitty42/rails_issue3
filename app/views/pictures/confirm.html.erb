<h2>以下の内容で送信します</h2>
<%= form_with(model: @picture,local: true,url: pictures_path) do |form| %>
    <%= form.hidden_field :image %>
    <%= form.hidden_field :content %>
    <%= image_tag(@picture.image.url) if @picture.image && @picture.image.url %>
    <p>Content:<%= @picture.content %></p>
    <%= form.submit '登録する' %>
<% end %>
 <%= form_with(model: @picture,url:new_picture_path,local:true,method: 'get')
    <%= form.hidden_field :image_cache %>
    <%= form.submit '戻る',name: 'back' %>
    <p>Content:<%= @picture.content %></p>
    
<% end %>
